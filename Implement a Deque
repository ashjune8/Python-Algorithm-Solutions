class Deque(object):
    def __init__(self):
        self.items = []
    def empty(self):
        return self.items == []
    def addfront(self,x):
        self.items.append(x)
    def addrear(self,x):
        self.items.insert(0,x)
    def remfront(self):
        self.items.pop()
    def remrear(self):
        self.items.pop(0)
    def size(self):
        return len(self.items)
